<Window x:Class="BookWiki.Presentation.Wpf.FileSystemWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BookWiki.Presentation.Wpf"
        xmlns:system="clr-namespace:System;assembly=System"
        xmlns:views="clr-namespace:BookWiki.Presentation.Wpf.Views"
        mc:Ignorable="d"
        KeyDown="OnKeyDown"
        Title="Книжная Полка" Height="800" Width="680">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="20"></RowDefinition>
        </Grid.RowDefinitions>
        <ScrollViewer VerticalScrollBarVisibility="Visible" Name="FileSystemScroll">
            
        </ScrollViewer>
    </Grid>
</Window>
