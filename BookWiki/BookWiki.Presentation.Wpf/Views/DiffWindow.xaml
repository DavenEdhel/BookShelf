<Window x:Class="BookWiki.Presentation.Wpf.Views.DiffWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BookWiki.Presentation.Wpf.Views"
        mc:Ignorable="d"
        Title="Скомпилировать главу" Height="900" Width="400">
    <StackPanel>
        <TextBlock Margin="10 0 0 0" FontFamily="Times New Roman" FontSize="16" Height="30">Тайтл:</TextBlock>
        <TextBox Margin="10 0 0 0" FontFamily="Times New Roman" FontSize="16" Height="30" Name="BookTitle"></TextBox>
        <CheckBox Margin="10 0 0 0" FontFamily="Times New Roman" FontSize="16" Height="30" Name="Shrink">Убрать повторения</CheckBox>
        <Button  FontFamily="Times New Roman" FontSize="16" Height="30" Click="GenerageFb2" Margin="10, 0">Компилировать</Button>
        <StackPanel Margin="0 10 0 0" Name="Chapters" Orientation="Vertical"></StackPanel>
    </StackPanel>
</Window>
